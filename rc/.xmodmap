! printscreen
!keycode 107 = XF86AudioMute

! pause
keycode 127 = XF86AudioRaiseVolume

! scroll lock
keycode 78 = XF86AudioLowerVolume
keycode 135 = Super_R

! Use Caps Lock button for compose key
clear lock
keycode 66 = Multi_key

!keysym XF86Back = Prior
!keysym XF86Forward = Next

! Shift-Space becomes Underscore
!keycode 65 = space underscore space
